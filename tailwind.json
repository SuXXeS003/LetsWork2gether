{
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"h-full": {
		"style": {
			"height": "100%"
		}
	},
	"w-full": {
		"style": {
			"width": "100%"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"flex-col": {
		"style": {
			"flexDirection": "column"
		}
	},
	"justify-between": {
		"style": {
			"justifyContent": "space-between"
		}
	},
	"bg-primary": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(23 22 55 / var(--tw-bg-opacity))"
		}
	},
	"bg-secondary-300": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(122 212 255 / var(--tw-bg-opacity))"
		}
	},
	"bg-secondary-600": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(0 151 223 / var(--tw-bg-opacity))"
		}
	},
	"p-6": {
		"style": {
			"paddingTop": 24,
			"paddingRight": 24,
			"paddingBottom": 24,
			"paddingLeft": 24
		}
	},
	"p-4": {
		"style": {
			"paddingTop": 16,
			"paddingRight": 16,
			"paddingBottom": 16,
			"paddingLeft": 16
		}
	},
	"text-xs": {
		"style": {
			"fontSize": 12
		}
	},
	"text-white": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(255 255 255 / var(--tw-text-opacity))"
		}
	}
}